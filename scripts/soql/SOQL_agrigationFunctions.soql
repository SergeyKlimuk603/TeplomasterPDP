
SELECT Id, Name, NumberField__c FROM TestObject__c WHERE Name LIKE 'ResearchSOQL%'

//Возвращает объекты AggregateResult. Количество возвращенных записей равно колчеству различных вариантов поля Name
SELECT COUNT(Id), COUNT(NumberField__c) numOfRecords, COUNT(Id) Ids, Name FROM TestObject__c WHERE Name LIKE 'ResearchSOQL%' GROUP BY Name

